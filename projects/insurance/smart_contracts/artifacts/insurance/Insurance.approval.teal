#pragma version 10

smart_contracts.insurance.contract.Insurance.approval_program:
    txn ApplicationID
    bnz main_entrypoint@2
    callsub __init__

main_entrypoint@2:
    callsub __puya_arc4_router__
    return


// smart_contracts.insurance.contract.Insurance.__puya_arc4_router__() -> uint64:
__puya_arc4_router__:
    proto 0 1
    txn NumAppArgs
    bz __puya_arc4_router___after_if_else@9
    method "create()void"
    method "register_asset(string,uint64,string,string)void"
    method "review_request(bool,string)void"
    method "get_asset_id()uint64"
    txna ApplicationArgs 0
    match __puya_arc4_router___create_route@2 __puya_arc4_router___register_asset_route@3 __puya_arc4_router___review_request_route@4 __puya_arc4_router___get_asset_id_route@5
    int 0
    retsub

__puya_arc4_router___create_route@2:
    txn OnCompletion
    !
    assert // OnCompletion is NoOp
    txn ApplicationID
    !
    assert // is creating
    callsub create
    int 1
    retsub

__puya_arc4_router___register_asset_route@3:
    txn OnCompletion
    !
    assert // OnCompletion is NoOp
    txn ApplicationID
    assert // is not creating
    txna ApplicationArgs 1
    extract 2 0
    txna ApplicationArgs 2
    btoi
    txna ApplicationArgs 3
    extract 2 0
    txna ApplicationArgs 4
    extract 2 0
    callsub register_asset
    int 1
    retsub

__puya_arc4_router___review_request_route@4:
    txn OnCompletion
    !
    assert // OnCompletion is NoOp
    txn ApplicationID
    assert // is not creating
    txna ApplicationArgs 1
    txna ApplicationArgs 2
    extract 2 0
    callsub review_request
    int 1
    retsub

__puya_arc4_router___get_asset_id_route@5:
    txn OnCompletion
    !
    assert // OnCompletion is NoOp
    txn ApplicationID
    assert // is not creating
    callsub get_asset_id
    itob
    byte 0x151f7c75
    swap
    concat
    log
    int 1
    retsub

__puya_arc4_router___after_if_else@9:
    int 0
    retsub


// smart_contracts.insurance.contract.Insurance.create() -> void:
create:
    proto 0 0
    byte "analyst"
    txn Sender
    app_global_put
    byte "asset_status"
