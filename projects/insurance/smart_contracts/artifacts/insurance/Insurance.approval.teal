    app_global_put
    retsub


// smart_contracts.insurance.contract.Insurance.register_asset(asset_name: bytes, asset_value: uint64, asset_type: bytes, asset_description: bytes) -> void:
register_asset:
    proto 4 0
    byte "asset_name"
    frame_dig -4
    app_global_put
    byte "asset_value"
    frame_dig -3
    app_global_put
    byte "asset_type"
    frame_dig -2
    app_global_put
    byte "asset_description"
    frame_dig -1
