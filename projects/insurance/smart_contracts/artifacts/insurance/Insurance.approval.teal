// smart_contracts.insurance.contract.Insurance.review_request(acceptance: bytes, analyst_comments: bytes) -> void:
review_request:
    proto 2 0
    txn Sender
    int 0
    byte "analyst"
    app_global_get_ex
    assert // check self.analyst exists
    ==
    assert
    int 0
    byte "asset_status"
    app_global_get_ex
    assert // check self.asset_status exists
    byte "accepted"
    ==
    !
    assert
    int 0
    byte "asset_status"
    app_global_get_ex
    assert // check self.asset_status exists
    byte "insured"
    ==
    !
    assert
    byte "analyst_comments"
    frame_dig -1
    app_global_put
    frame_dig -2
    byte 0x00
    !=
    bz review_request_else_body@3
    byte "asset_status"
    byte "accepted"
    app_global_put
    itxn_begin
    byte "Token for the asset"
    itxn_field Note
    int 0
    itxn_field ConfigAssetDefaultFrozen
    int 0
    itxn_field ConfigAssetDecimals
    int 1
    itxn_field ConfigAssetTotal
    byte "INS"
    itxn_field ConfigAssetUnitName
    byte "Insurance"
    itxn_field ConfigAssetName
    int acfg
    itxn_field TypeEnum
    int 0
    itxn_field Fee
    itxn_submit
    byte "asset_id"
    itxn CreatedAssetID
    app_global_put
    b review_request_after_if_else@4

review_request_else_body@3:
    byte "asset_status"
    byte "rejected"
    app_global_put

review_request_after_if_else@4:
    retsub


